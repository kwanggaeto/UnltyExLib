<?xml version="1.0" encoding="utf-8"?>
<socketSettings xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<servers>
		<server>
			<protocol>TCP</protocol>
			<maxConnection>5</maxConnection>
			<address>127.0.0.1</address>
			<port>1234</port>
			<listenable>false</listenable>
			<listenAtStart>false</listenAtStart>
			<override>false</override>
			<uniqueId>SocketServer</uniqueId>
		</server>
	</servers>
	<clients>
		<client>
			<protocol>TCP</protocol>
			<address>127.0.0.1</address>
			<port>1234</port>
			<connectable>false</connectable>
			<reconnectable>true</reconnectable>
			<reconnectInterval>1000</reconnectInterval>
			<connectAtStart>false</connectAtStart>
			<uniqueId>SocketClient</uniqueId>
		</client>
	</clients>
</socketSettings>